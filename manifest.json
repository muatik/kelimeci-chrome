{
	"name": "Kelimeci.net Sözlük",
	"description": "",
	"version": "0.1",
	"manifest_version": 1,
	"browser_action":{
		"default_title": "kelimeci.net Sözlük",
		"default_popup": "kelimeci.html",
		"default_icon": "kelimeci32.png"
	},
	"background_page": "extension.html",
	"content_scripts":[{
		"matches": ["http://*/*","https://*/*"],
		"js":["jquery.js","content.js","jquery.jplayer.js"],
		"css": ["content.css"],
		"all_frames": true
	}],
	"permissions":[
		"tabs","http://*/*", "https://*/*"
	],
	"icons":{
		"16":"kelimeci16.png",
		"32":"kelimeci16.png",
		"48":"kelimeci16.png",
		"64":"kelimeci48.png",
		"128":"kelimeci128.png"
	}

}
